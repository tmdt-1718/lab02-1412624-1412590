
<div class="container">
<h1>Inbox</h1>
<% @messages.each do |message| %>
  <div class="col-sm">
  <div class="card">
    <div class="card-body">
      <h4 class="card-title"><%= message.sender_id %></h4>
      <p class="card-text"><%= message.content %></p>
    </div>
  </div>
  </div>
  <% end %>
  
